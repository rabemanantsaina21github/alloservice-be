<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8"/>
        <meta content="IE=edge" http-equiv="X-UA-Compatible"/>
        <meta content="width=device-width, initial-scale=1" name="viewport"/>
        <title>
            {% block title %}Alloservice{% endblock %}
        </title>
        <link
        rel="icon" type="image/x-icon" href="{{ asset('images/allo_service.ico') }}"/>
        <!--css-->
        <link rel="stylesheet" media="screen" href="{{ asset('css/screen.css') }}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}"/>
        <link rel="stylesheet" media="all" href="{{ asset('css/responsive.css') }}"/>
        <link rel="stylesheet" type="text/css" href="{#{ asset('bootstrap/css/bootstrap.css') }#}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('fontawesome/css/all.min.css') }}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('fontawesome/css/fontawesome.min.css') }}"/>
        {% block stylesheets %}{% endblock %}
        <!--jQuery et javaScript-->
        <script type="text/javascript" src="{{ asset('jquery/jquery-3.3.1.min.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/popper.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.js') }}"></script>
        <script src="{{ asset('fontawesome/js/all.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('fontawesome/js/fontawesome.min.js') }}" type="text/javascript"></script>
    </head>
    <body style="background-color: #F4F5F6">
        <!--BODY-->
        {% block body %}{% endblock %}
        <script type="text/javascript" src="{{ asset('jquery/script.js') }}"> </script>
        <script type="text/javascript" src="{{ asset('js/alloservice-all-animation.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/alloservice-social-network.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/analytics.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/application.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/asset-composer.js') }}" charset="utf-8"></script>
        <script type="text/javascript" async="" src="{{ asset('js/bat.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/fbevents.js') }}" async=""></script>
        <script type="text/javascript" async="" src="{{ asset('js/hotjar.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/interferredEvents.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/jquery.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/modules.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/propper.js') }}"></script>
        <script type="text/javascript" async="" src="{{ asset('js/prum.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
    {% include "inc/footer.html.twig" %}
</html>